/*============================================================================
  Ajaxify.scss.liquid overrides
  - Make the drawer come from the right
==============================================================================*/
.ajaxify-drawer {
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    font-family: $secondaryFontStack;
    width: $cartWidthSmall;
    overflow: hidden;
    @include transition('right 0.5s cubic-bezier(0.75, 0.03, 0.52, 1)');

    @include at-query($min, $large) {
    width: $cartWidthLarge;
}
.ajaxifyCart--content {
    background-color: transparent;
    @include opacity(1);
    @include transition(none);
    @include transform(none);
}
.supports-csstransforms & {
    @include transition('transform 0.5s cubic-bezier(0.75, 0.03, 0.52, 1)');
}
.page-move--cart &,
.page-move--nav & {
    position:fixed;
    overflow: auto;
}
}
/*================ Ajaxify color overrides ================*/
.ajaxify-drawer {
    color: $colorMenuText;

    a {
    color: color-control($colorMenuText, 10%);

    &:hover {
        color: color-control($colorMenuText, 20%);
}

@include transition('all 0.3s ease');
}

h1 {
    font-size: em(24px);

    @include at-query($min, $large) {
    margin-bottom: $gutter;
}
}

h1, h2, h3, h4, h5, h6,
input[type="text"] {
    color: $colorMenuText;
}

label,
.cart__mini-labels {
    color: color-control($colorMenuText, 20%);
}

label {
    margin-bottom: ($gutter / 4);
}

textarea {
    background-color: $colorMenu;
    border-color: color-control($colorMenu, 15%);
    color: $colorMenuText;
}

.cart__product {
    a {
    color: color-control-text($colorMenuText, 20%);

    &:hover {
        color: color-control-text($colorMenuText, 25%);
}
}
}

.cart__row,
.ajaxifyCart--num,
.ajaxifyCart--add,
.ajaxifyCart--minus {
    border-color: color-control($colorMenu, 15%);
}

.js--qty-adjuster:hover,
.ajaxifyCart--qty-adjuster:hover,
.js--qty-adjuster:active,
.ajaxifyCart--qty-adjuster:active {
    background-color: color-control($colorMenu, 25%);
}
}

.ajaxifyCart--add,
.ajaxifyCart--minus {
    .add,
    .minus {
    color: $colorMenuText;
}

.js--qty-adjuster:hover &,
.ajaxifyCart--qty-adjuster:hover {
    color: red;
}
}


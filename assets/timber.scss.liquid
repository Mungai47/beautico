    .container {
        max-width: 1170px;
    }
}
@media (min-width:1300px) {
    .container {
        max-width: 1260px;
    }
}


/*============================================================================
  #Forms
==============================================================================*/
::-webkit-input-placeholder {
    color: $colorTextBody;
    opacity: 0.6;
}

:-moz-placeholder { /* Firefox 18- */
    color: $colorTextBody;
    opacity: 0.6;
}

::-moz-placeholder {  /* Firefox 19+ */
    color: $colorTextBody;
    opacity: 0.6;
}

:-ms-input-placeholder {
    color: $colorTextBody;
    opacity: 0.6;
}

::-ms-input-placeholder { /* Edge 12+ */
    color: $colorTextBody;
    opacity: 1;
}

form {
    margin: 0 0 ($gutter / 2);
}

select {
    font-size: 14px;
}

button,
input[type="text"],
input[type="search"],
input[type="password"],
input[type="email"],
input[type="file"],
input[type="number"],
input[type="tel"],
input[type="submit"],
textarea {
    -webkit-appearance: none;
    -moz-appearance: none;
}

input,
textarea,
select,
fieldset {
    max-width: 100%;

    &.input-full {
    width: 100%;
}
}

input,
select,
textarea {
    padding: ($gutter / 2) ($gutter * 0.45);
    line-height: 1.42;
}

input {
    padding: 12px 10px;
}

fieldset {
    border: $borderWidth solid $colorBorder;
    padding: $gutter / 2;
}

legend {
    border: 0;
    padding: 0;
}

optgroup {
    font-weight: bold;
}

input {
    display: inline-block;
    width: auto;
}

button,
input[type="submit"] {
    cursor: pointer;
}

input[type="submit"] {
    @extend .btn;
}
/*================ Input width and border ================*/
input[type="text"],
input[type="search"],
input[type="password"],
input[type="email"],
input[type="file"],
input[type="number"],
input[type="tel"],
textarea,
{
    color: $colorTextBody;
    border: $borderWidth solid $colorBorder;
    width: 100%;
    max-width: 100%;
    display: block;
    margin: 0 0 1em;

    .input-group & {
    border: 0;
    border-right: 1px solid $colorBorder;
}

&:focus, &:active{
    border: inherit solid darken($colorBorder, 10%);
  background-color: #fcfcfc;
}


&[disabled],
&.disabled {
    cursor: default;
    background-color: $disabledGrey;
    border-color: $disabledBorder;
}
}

textarea {
    min-height: 100px;
}

input[type="checkbox"],
input[type="radio"] {
    display: inline;
    margin: 0;
    padding: 0;
}

select {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    background-position: right center;
    background: {
        image: url($selectIcon);
    repeat: no-repeat;
    position: right 10px center;
    size: 10px;
}
padding-right: 28px;
text-indent: 0.01px;
text-overflow: '';
cursor: pointer;

/*================ Hide the svg arrow in IE9 and below ================*/
.ie9 &,
.lt-ie9 & {
    background-image: none;
    padding-right: 10px;
}
}

select::-ms-expand {
    display: none;
}

/*================ Form labels ================*/
label,
legend {
    color: $colorTextBody;
    display: inline-block;
    margin-bottom: 2px;
    font-family: $primaryFontStack;
    font-size: em(12px);
    font-weight: bold;

    &.inline {
    display: inline;
}
}

/*================ Accessible labels ====================*/

.visuallyhidden {
    @include visuallyHidden;
}

/*================ Off-screen labels with placeholder ================*/
.form-label--hidden {
    display: inline;  // reset margins and helps FF deal with the input on a SR

    .ie9 &,
    .lt-ie9 & {
    .visuallyhidden {
    clip: auto;
    height: auto;
    margin: 0;
    overflow: visible;
    position: static;
    width: auto;
}
}
}

/*================ We don't want the same label treatment for checkboxes/radios ================*/
input[type="checkbox"] + label,
input[type="radio"] + label {
    font-weight: normal;
}

label[for] {
    cursor: pointer;
}

.label-hint {
    color: #999;
}

/*================ Horizontal Form ================*/
form.form-horizontal,
.form-horizontal {
    margin-bottom: 0;

    input[type="text"],
    input[type="search"],
    input[type="password"],
    input[type="email"],
    input[type="file"],
    input[type="number"],
    input[type="tel"],
    textarea,
    select,
    label {
    display: inline-block;
    margin-bottom: 0;
    width: auto;
}
}

/*================ Error styles ================*/
input[type="text"],
input[type="search"],
input[type="password"],
input[type="email"],
input[type="file"],
input[type="number"],
input[type="tel"],
textarea {
    &.error {
    border-color: $errorRed;
    background-color: $errorRedBg;
    color: $errorRed;
}
}

label.error {
    color: $errorRed;
}


/*================ Input Group ================*/
.input-group {
    border: $borderWidth solid $colorBorder;

    input {
    // Nasty Firefox hack for inputs http://davidwalsh.name/firefox-buttons
        &::-moz-focus-inner {
        border: 0;
    padding: 0;
    margin-top: -1px;
    margin-bottom: -1px;
}
}
}

.input-group-field,
.input-group-btn {
    display: table-cell;
    vertical-align: middle;
    margin: 0;
}

.input-group .btn,
.input-group .input-group-field {
    height: 37px;
}

.input-group .input-group-field {
    width: 100%;
    margin-bottom: 0;
}

.input-group-btn {
    position: relative;
    white-space: nowrap;
    width: 1%;
    padding: 0;


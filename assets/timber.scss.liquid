    - jQuery FlexSlider v2.2.2 | http://www.woothemes.com/flexslider/
    - Contributing author: Tyler Smith (@mbmufffin)
==============================================================================*/
.flexslider {
    padding: 0;
    width:100%;
  	@include at-query ($max, $laptop){
      margin:0;
    }
  	@include at-query ($max, $postlaptop){
      margin:0;
    }
}
.flexslider .slider-subtitle{
  font-family: $primaryFontStack;
}
.flexslider .slider-title{
  font-family: $ternaryFontStack;
}
.flexslider .slider-button{
  font-family: $primaryFontStack;
}
.flexslider li { margin: 0; }
.flexslider .slides > li {
    display: none; /* Hide the slides before the JS is loaded. Avoids image jumping */
    margin: 0;
    position: relative;
    @include backface();
}
.flexslider .slides img {
    max-width: 100%;
    margin: 0 auto;
    display: block;
    width:100%;
}
.slides { @include clearfix; }
html[xmlns] .slides { display: block; }
* html .slides { height: 1%; }
.template-index .main-content .shopify-section.tt-flexslider {
    margin-top: 0;
}

/*================ No JS Fallback ================*/
.no-js .slides > li:first-child { display: block; }
.flexslider { position: relative; zoom: 1; }
.flex-viewport {
  max-height: 2000px;
  @include transition('all 1s ease');
}
.loading .flex-viewport { max-height: 300px; }
.flexslider .slides { zoom: 1; }
.carousel li { margin-right: 5px; }

/*================ Direction Nav ================*/
.flex-direction-nav {
    margin: 0;
    padding: 0;
    list-style: none;
  	float:left;
  	width:100%;
}
.flex-direction-nav .flex-next::before {
    content: "\f142";
    font-size: 26px;
    color: #9f9f9f;
    line-height: 34px;
    padding:0;
  	font-family: "Material Design Icons";
}
.flex-direction-nav .flex-prev::before {
    content: "\f141";
    font-size: 26px;
    color: #9f9f9f;
    line-height: 34px;
    padding:0;
  	font-family: "Material Design Icons";
}
.flex-direction-nav .flex-disabled {
    @include opacity('0 !important');
    filter: alpha(opacity=0);
    cursor: default;

}

.flex-direction-nav a {
	position:absolute;
  	border:2px solid #fff;
  	@include border-radius(50%);
    /*================ Hide SVG arrows in oldIE ================*/
    .lte-ie9 & {
    	display: none;
	}

&.flex-prev {
    // background-image: url("data:image/svg+xml;base64,");
    background-position: center left;
}

&.flex-next {
    // background-image: url("data:image/svg+xml;base64,");
    background-position: center right;
}
}

/*================ Control Nav ================*/
.flex-direction-nav {
	margin: auto;
	padding: 0;
	list-style: none;
	float: left;
	width: 100%;
	position: absolute;
	bottom: 0;
	top: 0;
  	height:40px;
}
.flex-direction-nav li {
	display: inline-block;
  	vertical-align:top;
}
.flex-direction-nav a.flex-prev {
	float: left;
	width: 40px;
	font-size: 0;
	height: 40px;
  	left:0;
  	right:auto;
  	text-align:center;
  	padding:0 5px;
  	@include at-query ($max, $tablet){
      	left:10px;
    }
  	@include at-query ($max, $mobile){
      	left:0;
    }
}
.flex-direction-nav a.flex-next {
	float: left;
	width: 40px;
	font-size: 0;
	height: 40px;
  	text-align:center;
  	right:0;
  	padding:0 6px;
  	left:auto;
  	@include at-query ($max, $tablet){
      	right:10px;
    }
  	@include at-query ($max, $mobile){
      	right:0;
    }
}
.flex-direction-nav a {
	display: inline-block;
	text-rendering: auto;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
  	@include opacity(0);
  	@include transition('400ms ease-in-out');
    @include at-query ($max, $tablet){
      	@include opacity(1);
    }
}
.flexslider:hover .flex-direction-nav a{
  	@include opacity(1);
}
.flexslider:hover .flex-direction-nav a.flex-prev {
    left: 40px;
    @include at-query ($max, $tablet){
      left: 20px;
    }
  	@include at-query ($max, $mobile){
      	left: 0;
    }
}
.flexslider:hover .flex-direction-nav a.flex-next {
    right: 40px;
    @include at-query ($max, $tablet){
      right: 20px;
    }
  	@include at-query ($max, $mobile){
      	right: 0;
    }
}
.flex-control-nav {
    position: absolute;
    bottom: 78px;
  	right:160px;
  	left:auto;
    width: auto;
    text-align: center;
    margin: 0;
    padding: 0;
    list-style: none;
  	z-index:1;
  	display:none;
  	@include at-query ($max, 1560px){
      right:110px;
    }
  	@include at-query ($max, $laptop){
      	right:130px;
    }
  	@include at-query ($max, $tablet){
      	bottom: 28px;
		right: 110px;
    }
  @include at-query ($max, $mobile){
      	bottom: 15px;
    right: 80px;
    }

    li {
    margin: 0 6px;
    display: inline-block;
    zoom: 1;
    *display: inline;
    vertical-align: middle;
}
}

.flex-control-paging li a {
	cursor: pointer;
  	@include transition('all 0.1s ease-in-out');
	font-size: 20px;
	line-height: 24px;
	font-weight: 500;
	color: #fff;
  	@include at-query ($max, $mobile){
      	font-size: 14px;
		line-height: 20px;
    }

&.flex-active {
    cursor: default;
  	font-size:30px;
  	font-weight:700;
  	color:#81ba00;
  @include at-query ($max, $mobile){
    	font-size: 20px;
  }
}
}

.flex-control-thumbs {margin: 5px 0 0; position: static; overflow: hidden;}
.flex-control-thumbs li {width: 25%; float: left; margin: 0;}
.flex-control-thumbs img {width: 100%; display: block; @include opacity(.7); cursor: pointer;}
.flex-control-thumbs img:hover {@include opacity(1);}
.flex-control-thumbs .flex-active {@include opacity(1); cursor: default;}

.flex-direction-nav a{
  	@include at-query ($max, $mobile){
        background:none;
      	border:none;
    }
}
.flex-direction-nav a:hover{
  	@include at-query ($max, $mobile){
        background:none;
      	border:none;
    }
}
.flex-direction-nav .flex-next::hover::before,.flex-direction-nav .flex-prev::hover::before{
    color:$slide_button_txt_hover_color;
}
@include at-query ($max, $mobile) {
  .flex-direction-nav .flex-next::before, .flex-direction-nav .flex-prev::before{
	color: #777;
  }
}

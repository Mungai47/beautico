    .icon {
    color: $colorTextBody;
    position: relative;
    font-size:18px;
    line-height:18px;
}
}
}

/*============================================================================
  #Icons
==============================================================================*/
.icon-fallback-text .icon {
    display: none;

    .supports-fontface & {
